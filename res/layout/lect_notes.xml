<?xml version="1.0" encoding="utf-8"?>
            <TableLayout
 xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:fillViewport="true"
  android:background="#d3d3d3">
    
<include layout="@layout/actionbar_layout"/>
    	<TableRow
    	    android:layout_width="fill_parent"
    	    android:layout_height="wrap_content"
    	    android:gravity="center_horizontal" >
    	    
    	    <TextView
                android:id="@+id/IT"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="15dip"
                android:text=" IT"
                android:textColor="#000000"
            	android:textSize="20dip"
            	android:textStyle="bold"/>
    	    </TableRow>
            
    	<TableRow
    	    android:layout_width="fill_parent"
    	    android:layout_height="wrap_content"
    	    android:gravity="center_horizontal" >
                            <TextView
                                android:id="@+id/link_to_fybtech"
                                android:layout_width="175dp"
                                android:layout_height="wrap_content"
                                android:background="#d3d3d3"
                                android:padding="18dip"
                                android:layout_weight="0.02"
                                android:text="F.Y.B.Tech"
                                android:textColor="#000000" />
                        
                            <TextView
                                android:id="@+id/link_to_sybtech_it"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="#d3d3d3"
                                android:padding="18dip"
                                android:layout_weight="0.02"
                                android:text="S.Y.B.Tech"
                                android:textColor="#000000" />
                            </TableRow>
                            
    	
    	<TableRow
    	    android:layout_width="fill_parent"
    	    android:layout_height="wrap_content"
    	    android:gravity="center_horizontal" >
                            
                            <TextView
                                android:id="@+id/link_to_tybtech_it" 
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="T.Y.B.Tech"
                                android:layout_weight="0.02"
                                android:background="#d3d3d3"
                                android:textColor="#000000"
                                android:padding="18dip" 
                                />

                            
                            <TextView
                                android:id="@+id/link_to_btech_it" 
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="B.tech"
                                android:layout_weight="0.02"
                                android:background="#d3d3d3"
                                android:textColor="#000000"
                                android:padding="18dip" 
                                />

                           </TableRow>
                          
                      
            <TableRow
    	    android:layout_width="fill_parent"
    	    android:layout_height="wrap_content"
    	    android:gravity="center_horizontal" >                

            <!-- connect to comps -->
            
            <TextView
                android:id="@+id/Comps"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text=" Computer Science"
                android:padding="15dip"
                android:textColor="#000000"
            	android:textSize="20dip"
            	android:textStyle="bold"/>
            </TableRow>
            
    	<TableRow
    	    android:layout_width="fill_parent"
    	    android:layout_height="wrap_content"
    	    android:gravity="center_horizontal" >
   
            <TextView
                                android:id="@+id/link_to_fybtech_comps"
                                android:text="F.Y.B.Tech" 
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.02"
                                android:background="#d3d3d3"
                                android:textColor="#000000"
                                android:padding="18dip"
                                
                                />

                        
            <TextView
                                android:id="@+id/link_to_sybtech_comps" 
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="S.Y.B.Tech"
                                android:layout_weight="0.02"
                                android:background="#d3d3d3"
                                android:textColor="#000000"
                                android:padding="18dip" 
                                />

                       </TableRow>
                       
    	<TableRow
    	    android:layout_width="fill_parent"
    	    android:layout_height="wrap_content"
    	    android:gravity="center_horizontal" >
           	<TextView
                                android:id="@+id/link_to_tybtech_comps" 
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="T.Y.B.Tech"
                                android:layout_weight="0.02"
                                android:background="#d3d3d3"
                                android:textColor="#000000"
                                android:padding="18dip" 
                                />

                            
                            <TextView
                                android:id="@+id/link_to_btech_comps"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" 
                                android:text="B.tech"
                                android:layout_weight="0.02"
                                android:background="#d3d3d3"
                                android:textColor="#000000"
                                android:padding="18dip" 
                                />
                                    
    </TableRow>  
            <SlidingDrawer
	    android:id="@+id/slidingDrawer2"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:content="@+id/content"
	    android:handle="@+id/handle" >
    
	

	    <ImageButton
	        android:id="@+id/handle"
	        android:src="@drawable/handle_pressed"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        />
	    
	    
	      
	       
	    
	    <TableLayout
	        android:id="@+id/content"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent" >
	        
	        
	         <TableRow
	            android:layout_height="wrap_content"
                            android:layout_width="fill_parent"
                            android:gravity="center_horizontal">
	  <TextView
	            android:id="@+id/link_to_login"
	            android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
	        	android:text=" logout"
	        	android:layout_gravity="center"
	        	android:textColor="#000000"
	        	android:textSize="15dip" />
	       </TableRow>
	        
	         <TableRow
	            android:layout_height="wrap_content"
                            android:layout_width="fill_parent"
                            android:gravity="center_horizontal">
	         <TextView
	            android:id="@+id/link_to_dashboard"
	            android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
	        	
	        	android:text="Home  "
	        	android:textColor="#000000"
	        	android:textSize="15dip" />
	        
	         <TextView
	            android:id="@+id/link_to_events"
	            android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
	        	android:text="    Events"
	        	android:textColor="#000000"
	        	android:textSize="15dip" />
	         </TableRow>
	         
	          <TableRow
	            android:layout_height="wrap_content"
                            android:layout_width="fill_parent"
                            android:gravity="center_horizontal">
	         <TextView
	            android:id="@+id/link_to_help"
	            android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
	        	android:text="Help  "
	        	android:textColor="#000000"
	        	android:textSize="15dip" />
	       
	         <TextView
	            android:id="@+id/link_to_lect_notes"
	            android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
	        	android:text="    Lecture Notes"
	        	android:textColor="#000000"
	        	android:textSize="15dip" />
	         </TableRow>
	          <TableRow
	            android:layout_height="wrap_content"
                            android:layout_width="fill_parent"
                            android:gravity="center_horizontal">
	         
	           <TextView
	            android:id="@+id/link_to_settings"
	            android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
	        	android:textSize="15dip"
	        	android:text="Settings  "
	        	android:textColor="#000000" />
	        
	         <TextView
	            android:id="@+id/link_to_assignments"
	            android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
	        	android:text="    Assignments"
	        	android:textColor="#000000"
	        	android:textSize="15dip" />
	         
	         </TableRow>
	         
	       
	       
	        
	    
	   </TableLayout>
	</SlidingDrawer>
                              
</TableLayout>

